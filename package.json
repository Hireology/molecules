{
  "name": "@hireology/molecules",
  "version": "1.7.4",
  "description": "Hireology's Component Library",
  "homepage": "https://github.com/Hireology/molecules#readme",
  "author": "Hireology Frontend Engineers",
  "license": "MIT",
  "main": "dist/molecules.js",
  "module": "dist/molecules.esm.js",
  "style": "dist/molecules.css",
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/Hireology/molecules/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hireology/molecules.git"
  },
  "scripts": {
    "build": "NODE_ENV=production rm -rf dist && rollup -c",
    "build-storybook": "build-storybook -c .storybook",
    "storybook": "start-storybook",
    "test": "NODE_ENV=test jest",
    "test-coverage": "NODE_ENV=test jest --coverage"
  },
  "dependencies": {
    "classnames": "2.2.5",
    "lodash.debounce": "^4.0.8",
    "lodash.pullall": "^4.2.0",
    "lodash.startswith": "^4.2.1",
    "prop-types": "^15.7.2",
    "react": "15.6.2",
    "react-dom": "15.6.2",
    "react-onclickoutside": "6.7.0",
    "react-portal": "4.2.1"
  },
  "peerDependencies": {
    "react": "15.6.2",
    "react-dom": "15.6.2",
    "prop-types": "^15.7.2",
    "classnames": "2.2.5"
  },
  "devDependencies": {
    "@babel/core": "^7.5.0",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-notes": "5.1.9",
    "@storybook/react": "5.1.8",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "css-loader": "1.0.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-15": "1.4.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "15.1.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.3.0",
    "jest-cli": "24.8.0",
    "node-sass": "4.12.0",
    "prettier": "1.16.4",
    "react-test-renderer": "15.6.2",
    "rollup": "^1.16.7",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-replace": "^2.2.0",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "setupFiles": [
      "<rootDir>/test_env.setup.js"
    ],
    "globals": {
      "__DEV__": "boolean"
    },
    "testRegex": "(.*)(-test).(js|jsx)$",
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
